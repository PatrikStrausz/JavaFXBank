<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewWindow">
   <children>
      <Pane fx:id="infoPane" prefHeight="66.0" prefWidth="900.0">
         <children>
            <Button fx:id="btnLogout" layoutX="822.0" layoutY="21.0" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="56.0" text="Logout" />
            <Button fx:id="btnChangePassword" layoutX="702.0" layoutY="21.0" mnemonicParsing="false" onAction="#openChangePassword" prefHeight="25.0" prefWidth="111.0" text="Change password" />
            <Button fx:id="btnLog" layoutX="638.0" layoutY="21.0" mnemonicParsing="false" onAction="#getLog" prefHeight="25.0" prefWidth="56.0" text="Log" />
            <Label fx:id="lblName" layoutX="30.0" layoutY="25.0" />
         </children></Pane>
      <ListView fx:id="lvLog" layoutX="684.0" layoutY="66.0" prefHeight="435.0" prefWidth="210.0" />
      <ListView fx:id="lvMessage" fixedCellSize="-10.0" layoutY="66.0" prefHeight="435.0" prefWidth="200.0">
         <effect>
            <Glow />
         </effect></ListView>
      <TextField fx:id="tfTo" layoutX="200.0" layoutY="66.0" prefHeight="25.0" prefWidth="485.0" promptText="To:" />
      <TextArea fx:id="areaMessage" layoutX="200.0" layoutY="91.0" prefHeight="375.0" prefWidth="485.0" promptText="Message:" />
      <Button fx:id="btnSendMessage" layoutX="199.0" layoutY="466.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="33.0" prefWidth="485.0" text="Send message" />
      <Label fx:id="lblMessageError" alignment="CENTER" contentDisplay="CENTER" layoutX="199.0" layoutY="439.0" prefHeight="17.0" prefWidth="485.0" textFill="RED">
         <font>
            <Font size="18.0" />
         </font></Label>
   </children>
</AnchorPane>
